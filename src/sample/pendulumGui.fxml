<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Text?>

<Pane prefHeight="400.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.PendulumGuiController">
    <Button fx:id="Start" layoutX="70.0" layoutY="75.0" mnemonicParsing="false" onAction="#onClickStart" prefHeight="26.0" prefWidth="64.0" text="Start" />
    <Button fx:id="Type" layoutX="70.0" layoutY="114.0" mnemonicParsing="false" onAction="#onClickType" text="Double" />
   <Slider blockIncrement="0.1" layoutX="32.0" layoutY="37.0" majorTickUnit="1.0" max="2.0" min="0.2" minorTickCount="4" showTickLabels="true" showTickMarks="true" snapToTicks="true" value="1.0" />
   <Text layoutX="31.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
   <TextField fx:id="L1_input" layoutX="47.0" layoutY="219.0" prefHeight="20.0" prefWidth="107.0" promptText="Length 1" />
   <TextField fx:id="PHI_input" layoutX="47.0" layoutY="252.0" prefHeight="20.0" prefWidth="107.0" promptText="Initial angle 1" />
   <TextField fx:id="M1_input" layoutX="47.0" layoutY="288.0" prefHeight="20.0" prefWidth="107.0" promptText="Mass 1" />
   <TextField fx:id="L2_input" layoutX="47.0" layoutY="323.0" prefHeight="26.0" prefWidth="107.0" promptText="Length 2" />
   <Button fx:id="Load" layoutX="75.0" layoutY="360.0" mnemonicParsing="false" onAction="#onClickLoad" text="Load" />
</Pane>
