<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="guiPane" prefHeight="400.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.agh.fis.anistratenko_team_project.Structure.StructureGuiController">
    <Slider fx:id="Speed" blockIncrement="0.1" layoutX="32.0" layoutY="37.0" majorTickUnit="1.0" max="2.0" min="0.2" minorTickCount="4" showTickLabels="true" showTickMarks="true" snapToTicks="true" value="1.0" />
    <Button fx:id="start" layoutX="32.0" layoutY="75.0" mnemonicParsing="false" onAction="#onClickStart" prefHeight="25.0" prefWidth="140.0" text="%button_start">
    <font>
        <Font size="12" />
    </font></Button>
   <Button fx:id="wave" layoutX="32.0" layoutY="111.0" mnemonicParsing="false" onAction="#onClickWave" prefHeight="26.0" prefWidth="140.0" text="Wave" />
   <Button fx:id="random" layoutX="32.0" layoutY="149.0" mnemonicParsing="false" onAction="#onClickRandom" prefHeight="26.0" prefWidth="140.0" text="Default" />

</Pane>
