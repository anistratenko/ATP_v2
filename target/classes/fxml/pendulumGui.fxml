<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Pane fx:id="GUIPane" prefHeight="450.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.agh.fis.anistratenko_team_project.Pendulum.PendulumGuiController">
    <Button fx:id="Start" layoutX="70.0" layoutY="75.0" mnemonicParsing="false" onAction="#onClickStart" prefHeight="26.0" prefWidth="64.0" text="Start">
      <font>
         <Font size="12.0" />
      </font></Button>
    <Button fx:id="Type" layoutX="70.0" layoutY="114.0" mnemonicParsing="false" onAction="#onClickType" text="Double">
      <font>
         <Font size="10.0" />
      </font></Button>
   <Slider fx:id="Speed" blockIncrement="0.1" layoutX="32.0" layoutY="37.0" majorTickUnit="1.0" max="2.0" min="0.2" minorTickCount="4" showTickLabels="true" showTickMarks="true" snapToTicks="true" value="1.0" />
   <Text layoutX="31.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
   <TextField fx:id="L1_input" layoutX="5." layoutY="155.0" prefHeight="20.0" prefWidth="80" promptText="Length 1" />
   <TextField fx:id="PHI_input" layoutX="5." layoutY="190.0" prefHeight="20.0" prefWidth="80" promptText="Angle 1" />
   <TextField fx:id="M1_input" layoutX="5." layoutY="225.0" prefHeight="20.0" prefWidth="80" promptText="Mass 1" />
   <TextField fx:id="L2_input" layoutX="100." layoutY="155.0" prefHeight="26.0" prefWidth="80" promptText="Length 2" />
    <TextField fx:id="THETA_input" layoutX="100." layoutY="190.0" prefHeight="20.0" prefWidth="80" promptText="Angle 2" />
    <TextField fx:id="M2_input" layoutX="100." layoutY="225.0" prefHeight="20.0" prefWidth="80" promptText="Mass 2" />

    <Button fx:id="Load" layoutX="75.0" layoutY="410.0" mnemonicParsing="false" onAction="#onClickLoad" text="Load">
      <font>
         <Font size="10.0" />
      </font></Button>
</Pane>
